      }
    },
    "services-auth-service": {
      "projectType": "library",
      "root": "libs/services/auth-service",
      "sourceRoot": "libs/services/auth-service/src",
      "prefix": "as",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/services/auth-service/tsconfig.lib.json",
